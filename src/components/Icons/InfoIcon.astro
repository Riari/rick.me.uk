---
const { width, height, style } = Astro.props;
---

<svg
    width={width ? width : "16px"}
    height={height ? height : "16px"}
    style={style}
    xmlns="http://www.w3.org/2000/svg"
    viewBox="0 0 24 24">
    <g
        fill="none"
        stroke="currentColor"
        stroke-linecap="round"
        stroke-linejoin="round">
        <circle stroke-width="1.5" cx="12" cy="12" r="10"></circle>
        <path stroke-width="2" d="M12 16v-4m0-4h.01"></path>
    </g>
</svg>
